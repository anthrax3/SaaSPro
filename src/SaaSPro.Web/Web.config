  <!-- NServiceBus -->
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="error" />
  <UnicastBusConfig ForwardReceivedMessagesTo="audit">
    <MessageEndpointMappings>
      <add Assembly="SaaSPro.DocumentService.Messages" Endpoint="SaaSPro.DocumentService.Host" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <entityFramework>
